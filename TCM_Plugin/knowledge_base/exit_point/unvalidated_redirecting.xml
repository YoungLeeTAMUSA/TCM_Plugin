<?xml version="1.0" encoding="UTF-8"?>
<exitpoints xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="exit_point_schema.xsd">
	<!-- -1 Anything is valid. -->
	<!-- 0 Only sanitized values are valid. -->
	<!-- 1 LITERAL and sanitized values are valid. -->
	<!-- X1;X2;... These are the accepted values. -->

	<!-- javax.servlet.http.HttpServletResponse -->
	<exitpoint id="01">
		<qualifiedname>javax.servlet.(http.Http)?ServletResponse</qualifiedname>
		<methodname>sendRedirect</methodname>
		<parameters type="java.lang.String" rules="1" />
	</exitpoint>
	<!-- javax.servlet.ServletContext -->
	<exitpoint id="02">
		<qualifiedname>javax.servlet.ServletContext</qualifiedname>
		<methodname>getRequestDispatcher</methodname>
		<parameters type="java.lang.String" rules="1" />
	</exitpoint>
</exitpoints>